<div id="body">
  <div class="wrapper">
    <div id="main" class="profile grid-a">
      <div id="content" class="resume hresume">
        <div class="profile-header">
          <div class="image" id="profile-picture">
            <img src={{pictureUrl}} class="photo" width="100" height="100" alt="{{firstName}} {{lastName}}">
          </div><h1><span id="name" class="n fn"><span class="given-name">Yevgeniy</span> <span class="family-name">Brikman</span></span></h1>
          <p class="title">
            {{headline}}
          </p>
          <dl id="headline" class="demographic-info adr">
            <dt>
              Location
            </dt>
            <dd class="locality">
              {{location.name}}
            </dd>
            <dt>
              Industry
            </dt>
            <dd class="industry">
              {{industry}}
            </dd>
          </dl>
          <h2 class="section-title">{{firstName}} {{lastName}}'s Overview </h2>
          <dl id="overview">
            {{#with positions.values}}
            <dt class="summary-current">
              Current
            </dt>
            <dd class="summary-current">
              <ul class="current">
                <li>
                  {{this.0.title}}<span class="at"> at </span>{{this.0.company.name}}
                </li>
              </ul>
            </dd>
            <dt class="summary-past">
              Past
            </dt>
            <dd class="summary-past">
              <ul class="past">
                <li>
                  {{1.title}}<span class="at"> at </span>{{1.company.name}}
                </li>
                <li>
                  {{2.title}}<span class="at"> at </span>{{2.company.name}}
                </li>
                <li>
                  {{3.title}}<span class="at"> at </span>{{3.company.name}}
                </li>
              </ul>
            </dd>
            {{/with}}

            <dt class="summary-education">
              Education
            </dt>
            <dd class="summary-education">
              <ul>
                {{#each educations.values}}
                  <li>{{schoolName}}</li>
                {{/each}}
              </ul>
            </dd>
            <dt>
              Connections
            </dt>
            <dd class="overview-connections">
              <p>
                <strong>{{connections._total}}</strong>
                connections
              </p>
            </dd>
            <dt class="websites">
              Websites
            </dt>
            <dd class="websites">
              <ul>
                {{#each memberUrlResources.values}}
                  <li>
                    <a href={{url}} target="_blank">{{name}}</a>
                  </li>
                {{/each}}
              </ul>
            </dd>
          </dl>
        </div>
        <div class="section" id="profile-summary">
          <div class="header">
            <h2>{{firstName}} {{lastName}}'s Summary </h2>
          </div>
          <div class="content">
            <p>
              {{summary}}
            </p>
            <div id="profile-specialties">
              <h3>Specialties</h3>
              <p>
                {{specialties}}
              </p>
            </div>
          </div>
        </div>
        <div class="section" id="profile-skills">
          <div class="header">
            <h2>{{firstName}} {{lastName}}'s Skills</h2>
          </div>
          <div class="content">
            <ul class="skills competencies">
              {{#each skills.values}}
              <li class="competency skill">
                <h3><a href="#">{{skill.name}}</a></h3>
                <span class="proficiency">({{proficiency.proficiency}}, {{proficiency.years}} years experience)</span>
              </li>
              {{/each}}
            </ul>
          </div>
        </div>
        <div class="section" id="profile-experience">
          <div class="header">
            <h2>{{firstName}} {{lastName}}'s Experience</h2>
          </div>
          <div class="content vcalendar">
            {{#each positions.values}}
            <div class="position first experience vevent vcard current-position">
              <a class="include" href="#name"></a>
              <div class="postitle">
                <h3 class="position-title anet"><span class="title">{{title}}</span></h3>
                {{#with company}}
                <h4><strong><a class="company-profile-public" href="#"><span class="org summary">{{name}}</span></a></strong></h4>
              </div>
              <p class="orgstats organization-details">
                {{type}}; {{industry}} Industry
              </p>
              {{/with}}
              <p class="period">
              {{#with startDate}}
                <abbr class="dtstart" title="{{year}}-{{month}}-01">
                  {{month}} {{year}}
                </abbr>
                {{/with}}
                &#8211;
                {{#if isCurrent}}
                <abbr class="dtstamp" title="2011-04-18">
                  Present
                </abbr>
                {{else}}
                {{#with endDate}}
                <abbr class="dtstamp" title="{{year}}-{{month}}-01">
                  {{month}} {{year}}
                </abbr>
                {{/with}}
                {{/if}}
              </p>
              <p class="desc">
                {{summary}}
              </p>
            </div>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </div>
</div>

